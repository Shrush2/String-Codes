import java.util.* ;
import java.io.*; 
import java.util.ArrayList;

public class Solution {

	public static ArrayList<ArrayList<Character>> interestingPattern(int N) {

		ArrayList<ArrayList<Character>>pattern = new ArrayList<>();

		for(int i=0;i<N;i++){
			ArrayList<Character>row = new ArrayList<>();
			char ch = (char)('A'+N-1-i);

			for(int j = 0; j<=i ;j++){
				row.add((char)(ch+j));
			}
			pattern.add(row);
		}
			return pattern;
	}
}

//pattern like
E
DE
CDE
BCDE
ABCDE
